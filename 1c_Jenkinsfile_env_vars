pipeline {
    agent any
    environment {
        TEST_ENV = 'dev'
    }
    stages {
        stage('Build') {
            environment {
                TEST_ENV = 'qa'
            }
            steps {
                sh 'printenv'
                // TEST_ENV will be qa
                sh 'echo $TEST_ENV inside Build Stage'
                echo "Running ${env.BUILD_ID} on ${env.JENKINS_URL}"
            }
        }
        stage('CheckEnv') {
            steps {
                // TEST_ENV will be dev
                sh 'echo $TEST_ENV inside CheckEnv'
            }
        }
    }
}