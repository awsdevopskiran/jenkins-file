pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                echo 'This is Build Stage.'
                echo 'This is Build 2.'
            }
        }
        stage('Test') {
            steps {
                echo 'This is Test 2st Stage.'
            }
        }
        stage('Deploy') {
            steps {
                echo 'This is Deploy 2st Stage.'
            }
        }
    }
}
